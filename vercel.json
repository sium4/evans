{
  "version": 2,
  "builds": [
    { "src": "frontend-main/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "^/(.*)", "dest": "/frontend-main/$1" }
  ]
  ,"headers": [
    {
      "source": "/:path*.js",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
